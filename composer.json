{
    "name": "mortezamasumi/fb-message",
    "description": "This is my package fb-message",
    "keywords": [
        "mortezamasumi",
        "laravel",
        "fb-message"
    ],
    "homepage": "https://github.com/mortezamasumi/fb-message",
    "support": {
        "issues": "https://github.com/mortezamasumi/fb-message/issues",
        "source": "https://github.com/mortezamasumi/fb-message"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Morteza Masumi",
            "email": "morteza.masumi@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "bezhansalleh/filament-shield": "^4.0",
        "filament/filament": "^4.0",
        "filament/spatie-laravel-media-library-plugin": "^4.0",
        "spatie/laravel-package-tools": "^1.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^8.0",
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "pestphp/pest-plugin-livewire": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Mortezamasumi\\FbMessage\\": "src/",
            "Mortezamasumi\\FbMessage\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mortezamasumi\\FbMessage\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mortezamasumi\\FbMessage\\FbMessageServiceProvider"
            ],
            "aliases": {
                "FbMessage": "Mortezamasumi\\FbMessage\\Facades\\FbMessage"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
